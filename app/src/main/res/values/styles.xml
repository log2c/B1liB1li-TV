<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AlignEndDialogTheme" parent="ThemeOverlay.AppCompat.Dark">
        <!--        <item name="android:windowBackground">@null</item>-->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:layout_margin">0dp</item>
        <item name="android:padding">0dp</item>
        <item name="android:layout_gravity">end</item>
        <item name="android:titleTextStyle">@style/TextAppearance.AppCompat.Widget.ActionBar.Title
        </item>
        <item name="android:subtitleTextStyle">
            @style/TextAppearance.AppCompat.Widget.ActionBar.Subtitle
        </item>
    </style>
</resources>